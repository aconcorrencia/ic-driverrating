<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/wallet_holo_blue_light"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvTituloCons"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="CONSUMO DE COMBUSTÍVEL"
            android:textColor="@color/wallet_link_text_light"
            android:textSize="16dp"
            android:textStyle="bold" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaCons"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassCons"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/wallet_holo_blue_light"
        android:orientation="horizontal"
        android:layout_marginTop="7dp">

        <TextView
            android:id="@+id/tvTituloCO2"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/wallet_link_text_light"
            android:textStyle="bold"
            android:textSize="16dp"
            android:text="DIÓXIDO DE CARBONO (CO2)" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaCO2"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassCO2"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/wallet_holo_blue_light"
        android:orientation="horizontal"
        android:layout_marginTop="7dp">

        <TextView
            android:id="@+id/tvTituloVeloc"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/wallet_link_text_light"
            android:textStyle="bold"
            android:textSize="16dp"
            android:text="VELOCIDADE" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaVeloc"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassVeloc"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/wallet_holo_blue_light"
        android:orientation="horizontal"
        android:layout_marginTop="7dp">

        <TextView
            android:id="@+id/tvTituloAcelLong"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/wallet_link_text_light"
            android:textStyle="bold"
            android:textSize="16dp"
            android:text="ACELERAÇÃO LONGITUDINAL" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaAccLong"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassAccLong"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/wallet_holo_blue_light"
        android:orientation="horizontal"
        android:layout_marginTop="7dp">

        <TextView
            android:id="@+id/tvTituloAcelTrans"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/wallet_link_text_light"
            android:textStyle="bold"
            android:textSize="16dp"
            android:text="ACELERAÇÃO TRANSVERSAL" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaAccTrans"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassAccTrans"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:background="@color/black"
        android:orientation="horizontal"
        android:layout_marginTop="7dp">

        <TextView
            android:id="@+id/tvTituloGeral"
            android:layout_height="wrap_content"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textStyle="bold"
            android:textSize="16dp"
            android:text="RESULTADO GERAL" />

    </LinearLayout>

    <TextView
        android:text="Nota: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvNotaGeral"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:text="Classificação: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassGeral"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"/>

    <TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvClassGeral"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/btnRetornarGerais"
        android:layout_width="match_parent"
        android:layout_height="37dp"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp"
        android:background="@color/colorPrimary"
        android:onClick="btnRetornarGerais"
        android:text="Retornar"
        android:textColor="@color/white"/>

</LinearLayout>